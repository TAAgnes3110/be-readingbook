meta {
  name: Create Category
  type: http
  seq: 1
}

post {
  url: http://0.0.0.0:9000/api/admin/categories
  body: json
  auth: inherit
}

headers {
  Content-Type: application/json
  Authorization: Bearer {{authToken}}
}

body:json {
  {
    "name": "Science Fiction",
    "description": "Science fiction books",
    "icon": "https://example.com/sci-fi-icon.png"
  }
}

settings {
  encodeUrl: true
  timeout: 0
}
